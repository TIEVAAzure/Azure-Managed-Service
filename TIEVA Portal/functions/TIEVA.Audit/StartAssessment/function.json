{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": ["post"],
      "route": "assessments/start"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "AssessmentQueue",
      "queueName": "assessment-queue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
